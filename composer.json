{
    "name": "hieofone-directory/hieofone-directory",
    "description": "Health Information Exchange of One Directory Server.",
    "keywords": [
        "hieofone",
        "laravel",
        "uma",
        "oauth2",
        "oidc",
        "php",
        "javascript",
        "health",
        "fhir",
        "medical"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Michael Shihjay Chen",
            "email": "shihjay2@gmail.com",
            "homepage": "http://noshemr.wordpress.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "bshaffer/oauth2-server-httpfoundation-bridge": "^1.3",
        "bshaffer/oauth2-server-php": "^1.10",
        "cache/filesystem-adapter": "^1.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.0",
        "google/apiclient": "~2.0",
        "graham-campbell/markdown": "^11.1",
        "jenssegers/date": "*",
        "knplabs/github-api": "^2.1",
        "laravel/framework": "^6.0",
        "laravel/socialite": "^4.0",
        "laravelcollective/html": "^6.0.0",
        "adci/full-name-parser": "^0.2.4",
        "php-http/guzzle6-adapter": "^1.1",
        "phpseclib/phpseclib": "~2.0",
        "shihjay2/openid-connect-uma-php": "dev-master",
        "simplesoftwareio/simple-qrcode": "~1.4",
        "socialiteproviders/generators": "^4.1",
        "socialiteproviders/manager": "^3.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5",
        "symfony/css-selector": "2.8.*|3.1.*",
        "symfony/dom-crawler": "2.8.*|3.1.*",
        "nunomaduro/collision": "^2.0",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "SocialiteProviders\\Doximity\\": "SocialiteProviders/Doximity/src/",
            "DIDKit\\": "DIDKit/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
